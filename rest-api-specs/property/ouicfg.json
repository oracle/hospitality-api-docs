          {
            "name": "hotelId",
            "in": "path",
            "required": true,
            "type": "string",
            "description": "Hotel Code"
          },
          {
            "name": "Trigger Property Refresh",
            "in": "body",
            "required": true,
            "description": "Request to refresh Property.",
            "schema": {
              "allOf": [
                {
                  "$ref": "#/definitions/refreshProperty"
                }
              ]
            }
          },
          {
            "$ref": "#/parameters/authKey"
